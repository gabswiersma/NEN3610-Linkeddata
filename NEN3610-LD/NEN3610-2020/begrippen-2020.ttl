	dcterms:source referentie:NEN3610-2020-3;
.
nen3610_begrip:Metadata a skos:Concept;
	skos:inScheme begrippenkaders:nen3610;
	rdfs:label "Metadata"@nl;
	rdfs:label "Metadata"@en;
	skos:prefLabel "Metadata"@nl;
