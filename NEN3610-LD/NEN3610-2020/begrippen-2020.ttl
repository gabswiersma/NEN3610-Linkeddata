	rdfs:label "Informatiemodel"@nl;
	rdfs:label "Conceptual model"@en;
	skos:altLabel "Conceptual schema"@en;
	skos:prefLabel "Informatiemodel"@nl;
	skos:definition "Formele beschijving van een beschouwingsgebied."@nl;
	skos:relatedMatch iso211_referentie:68;
